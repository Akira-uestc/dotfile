#workspaces button {
    padding: 5px;
    /*color: #313244;*/
    color: #a6adc8;
    margin-right: 5px;
}

#workspaces button.visible {
    /*color: #a6adc8;*/
    color: #343742;
    background: #a6e3a1;
    border-radius: 5px;
}

#workspaces button.focused {
    color: #a6adc8;
    /*background: #11111b;*/
    border-radius: 5px;
}

#workspaces button.urgent {
    color: #11111b;
    background: #a6e3a1;
    border-radius: 5px;
}

#workspaces button:hover {
    /*background: #11111b;*/
    /*color: #cdd6f4;*/
    background: #a6e3a1;
    color: #343742;
    border-radius: 5px;
}

